{
	"name": "scontainers",
	"version": "0.0.1",
	"description": "A container/collection/iterator library for JavaScript, comfortable to use, performant and versatile.",
	"main": "index.js",
	"scripts": {
		"test": "mocha --require babel-core/register test/index.js",
		"build-watch": "npx babel --watch src --out-dir dist",
		"prepare": "babel src --out-dir dist"
	},
	"keywords": [
		"scontainers",
		"containers",
		"collections",
		"iterator",
		"functional",
		"range",
		"traits",
		"straits"
	],
	"author": "peoro",
	"license": "ISC",
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-plugin-transform-function-bind": "^6.22.0",
		"babel-plugin-transform-strict-mode": "^6.24.1",
		"chai": "^4.2.0",
		"mocha": "^5.2.0",
		"straits-babel": "^1.0.0",
		"stream-buffers": "^3.0.2"
	},
	"dependencies": {
		"esast": "^0.1.0",
		"straits": "^1.0.0"
	},
	"babel": {
		"parserOpts": {
			"parser": "straits-babel"
		},
		"plugins": [
			"straits-babel/plugin",
			"transform-strict-mode",
			"transform-function-bind"
		]
	}
}
